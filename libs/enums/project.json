{
  "name": "enums",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/enums/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/enums",
